  long_name = air temperature two timesteps back
  units = K
  dimensions = (horizontal_loop_extent,vertical_layer_dimension)
  type = real
  kind = kind_phys
  intent = inout
  optional = F
[qp]
  standard_name = specific_humidity_two_timesteps_back
