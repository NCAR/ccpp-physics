  units = none
  dimensions = (horizontal_loop_extent)
  type = real
  kind = kind_phys
  intent = out
  optional = True
[conv_act_m]
  standard_name = consecutive_calls_for_grell_freitas_mid_level_convection
  long_name = Memory counter for GF midlevel
  units = none
  dimensions = (horizontal_loop_extent)
  type = real
  kind = kind_phys
  intent = out
  optional = True
[rrfs_sd]
