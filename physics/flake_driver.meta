  units = mm
  dimensions = (horizontal_loop_extent)
  type = real
  kind = kind_phys
  intent = inout
  optional = F
[lakedepth]
  standard_name = lake_depth
  long_name = lake depth
  units = m
  dimensions = (horizontal_loop_extent)
  type = real
  kind = kind_phys
  intent = in
  optional = F
[lake]
  standard_name = flag_nonzero_lake_surface_fraction
