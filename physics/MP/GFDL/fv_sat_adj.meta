  units = none
  dimensions = ()
  type = logical
  intent = in
[kmp]
  standard_name = top_layer_index_for_fast_physics
  long_name = top_layer_inder_for_gfdl_mp
  units = index
  dimensions = ()
  type = integer
  intent = in
[nwat]
