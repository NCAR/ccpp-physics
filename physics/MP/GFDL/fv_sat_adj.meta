  long_name = number of water species
  units = count
  dimensions = ()
  type = integer
  intent = in
[ngas]
  standard_name = number_of_gases_for_multi_gases_physics
  long_name = number of gases for multi gases physics
  units = count
  dimensions = ()
  type = integer
  intent = in
[rilist]
  standard_name = gas_constants_for_multi_gases_physics
